{
  "name": "manual-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettify": "prettier --write",
    "lint": "eslint",
    "lint-all": "eslint \"**/*.js\"",
    "prettify-all": "prettier --write \"./**/*.{js,json}\""
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "next": "9.4.1",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-icons": "^3.10.0"
  },
  "devDependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.1.0",
    "next-optimized-images": "^2.6.0",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "npm run prettify",
      "npm run lint"
    ],
    "*.{json}": [
      "npm run prettify"
    ]
  }
}
